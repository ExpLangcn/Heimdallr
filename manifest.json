{
    "name": "Heimdallr",
    "author": "Ghroth",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Identify fingerprint & Anti honeypot",
    "icons": { 
      "16": "resource/img/icon/ico.png",
      "48": "resource/img/icon/ico.png",
     "128": "resource/img/icon/ico.png" 
    },
    "background": {
      "persistent": true,
      "page": "resource/html/background.html"
    },
    "browser_action": {
      "default_title": "Heimdallr",
      "default_icon": "resource/img/icon/ico.png",
      "default_popup": "resource/html/popup.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "contextMenus",
        "browsingData",
        "debugger",
        "http://*/*",
        "https://*/*"
      ],
    "content_security_policy": "style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval'; object-src 'self';",
    "homepage_url": "https://github.com/graynjo/Heimdallr"
}